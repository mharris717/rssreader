#header
  = link_to 'Home', posts_path
  - if current_user
    |
    = link_to 'Refresh', {:controller => :feeds, :action => :refresh}, :method => :post
    |
    = link_to 'Manage Feeds', new_feed_path, :id => 'add-feed'
    |
    = link_to 'Logout', "/users/sign_out"
    = render :partial => 'posts/options'